{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "HealthCheck": {
    "Status": {
      "HealthCheckProbeType": "HttpStatus",
      "Port": 80,
      "EndPoint": "healthcheck/status"
    },
    "Startup": {
      "HealthCheckProbeType": "TcpProbe",
      "Port": 8880
    },
    "Readiness": {
      "HealthCheckProbeType": "TcpProbe",
      "Port": 8881
    },
    "Liveness": {
      "HealthCheckProbeType": "HttpProbe",
      "Port": 80,
      "EndPoint": "healthcheck/liveness"
    }
  }
}
