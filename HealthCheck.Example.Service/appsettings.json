{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "HealthCheck": {
    "Logging": {
      "LogProbe" : true,
      "LogStatusWhenHealthy" : false,
      "LogStatusWhenNotHealthy" : true
    },
    "HttpProbe": {
      "Port": 8080,
      "Endpoints": {
        "Status": "health/status",
        "Startup": "health/startup",
        "Readiness": "health/readiness",
        "Liveness": "health/liveness"
      }
    },
    "TcpProbe": {
      "Ports": {
        "Startup": 8081,
        "Readiness": 8081,
        "Liveness": 8081
      }
    }
  }
}
